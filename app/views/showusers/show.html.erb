    <link rel="stylesheet" href="css/bootstrap-image-gallery.min.css">
</head>

<body>
    <header>
        <link rel="stylesheet" href="//blueimp.github.io/Gallery/css/blueimp-gallery.min.css">
        <link rel="stylesheet" href="css/bootstrap-image-gallery.min.css">
    </header>
    
    
    
    
    <div><h1><%= @user.hitokoto_comment %></h1></div>
    
    <a href="<%= asset_path "photo01.jpg" %>" data-gallery>
  <img src="<%= asset_path "photo01.jpg" %>" class="img-responsive" style="margin-bottom:30px;">
</a>
