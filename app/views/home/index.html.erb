        </div>
      </div>
      <div class="item">
        <%= image_tag "slick/Silent Nights.jpg" %>
        <div class="carousel-caption">
          <h1>写真3</h1>
        </div>
      </div>
      <div class="item">
        <%= image_tag "slick/SP6 Complete.jpg" %>
